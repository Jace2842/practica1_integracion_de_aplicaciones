{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Proveedor",
  "description": "Representa un proveedor de productos para Fresh&Go",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "nombre": {
      "type": "string"
    },
    "contacto": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    }
  },
  "required": [
    "id",
    "nombre"
  ]
}